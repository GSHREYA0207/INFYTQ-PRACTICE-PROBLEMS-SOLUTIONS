def list_of_count(word_list):
    #start writing your code here
    count_list=[]
    for i in word_list:
        count=0
        for j in i:
            count+=1 
        count_list.append(count)
        
    
    return count_list

word_list=["COme","here"]
print(list_of_count(word_list))
