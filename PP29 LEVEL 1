def exchange_list(number_list):
    #start writing your code here
    l=[]
    n=len(number_list)
    s=0
    for i in range(0,len(number_list)):
        if(i<(n/2+1)):
            l.append(number_list[n-1])
            n-=1
        else:
            l.append(number_list[s])
            s+=1
    number_list=l

    
    return number_list
     
number_list=[1,2,3,4,5,6]
print(exchange_list(number_list))
